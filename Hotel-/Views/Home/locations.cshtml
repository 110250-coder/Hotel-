@using Hotel_.Databases
@model List<Locaties>

@{
    ViewData["Title"] = "Locations";
}

<section id="locaties">
    <div class="container">
    <h2>Locations</h2>
    @foreach (Locaties locatie in Model)
    {
        <a href="location/@locatie.Id">
                <h1>@locatie.Stad</h1>
        </a>

    }
    </div>
</section>